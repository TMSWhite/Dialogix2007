<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->
<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <application>
        <resource-bundle>
            <base-name>org.dialogix.main.Bundle</base-name>
            <var>msgs</var>
        </resource-bundle>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>es</supported-locale>
        </locale-config>
    </application>
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>org.dialogix.main.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>selectPage</property-name>
            <value>Logo</value>
        </managed-property>
        <managed-property>
            <property-name>userId</property-name>
            <value>0</value>
        </managed-property>
        <managed-property>
            <property-name>version</property-name>
            <value>0.1</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>org.dialogix.main.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>org.dialogix.main.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Header1</managed-bean-name>
        <managed-bean-class>org.dialogix.main.Header1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Footer1</managed-bean-name>
        <managed-bean-class>org.dialogix.main.Footer1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>TopNavigation1</managed-bean-name>
        <managed-bean-class>org.dialogix.main.TopNavigation1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>LeftNavigation1</managed-bean-name>
        <managed-bean-class>org.dialogix.main.LeftNavigation1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>MainMenu</managed-bean-name>
        <managed-bean-class>org.dialogix.main.MainMenu</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/MainMenu.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Instruments</from-outcome>
            <to-view-id>/Instruments.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>InstrumentLogic</from-outcome>
            <to-view-id>/InstrumentLogic.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>Instruments</managed-bean-name>
        <managed-bean-class>org.dialogix.main.Instruments</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ListInstruments</managed-bean-name>
        <managed-bean-class>org.dialogix.main.ListInstruments</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>InstrumentLogicFile</managed-bean-name>
        <managed-bean-class>org.dialogix.main.InstrumentLogicFile</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ListInstrumentSessions</managed-bean-name>
        <managed-bean-class>org.dialogix.main.ListInstrumentSessions</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>DataExporter</managed-bean-name>
        <managed-bean-class>org.dialogix.export.DataExporter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Export</managed-bean-name>
        <managed-bean-class>org.dialogix.main.Export</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Logo</managed-bean-name>
        <managed-bean-class>org.dialogix.main.Logo</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>InstrumentSessionRecap</managed-bean-name>
        <managed-bean-class>org.dialogix.main.InstrumentSessionRecap</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>InstrumentTranslationFile</managed-bean-name>
        <managed-bean-class>org.dialogix.main.InstrumentTranslationFile</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>DialogixParserTool</managed-bean-name>
        <managed-bean-class>org.dialogix.util.DialogixParserTool</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Login1_1</managed-bean-name>
        <managed-bean-class>org.dialogix.main.Login1_1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>UserManual</managed-bean-name>
        <managed-bean-class>org.dialogix.main.UserManual</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>LoadInstruments</managed-bean-name>
        <managed-bean-class>org.dialogix.main.LoadInstruments</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Admin</managed-bean-name>
        <managed-bean-class>org.dialogix.main.Admin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Status</managed-bean-name>
        <managed-bean-class>org.dialogix.main.Status</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PasteInstrument</managed-bean-name>
        <managed-bean-class>org.dialogix.main.PasteInstrument</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ParserTest</managed-bean-name>
        <managed-bean-class>org.dialogix.main.ParserTest</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Main</managed-bean-name>
        <managed-bean-class>org.dialogix.main.Main</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>InstrumentLogicFile_JSF</managed-bean-name>
        <managed-bean-class>org.dialogix.main.InstrumentLogicFile_JSF</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AnswerList</managed-bean-name>
        <managed-bean-class>org.dialogix.main.AnswerList</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
