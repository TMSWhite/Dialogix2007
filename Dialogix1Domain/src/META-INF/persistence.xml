<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="Dialogix1DomainPU" transaction-type="RESOURCE_LOCAL">
    <provider>oracle.toplink.essentials.PersistenceProvider</provider>
    <class>org.dialogix.model1.V1DataElement</class>
    <class>org.dialogix.model1.V1InstrumentSession</class>
    <class>org.dialogix.model1.V1ItemUsage</class>
    <properties>
      <property name="toplink.jdbc.user" value="dialogix6"/>
      <property name="toplink.jdbc.password" value="dialogix6_pass"/>
      <property name="toplink.jdbc.url" value="jdbc:mysql://localhost:3306/dialogix6?useUnicode=yes&amp;characterEncoding=UTF-8"/>
      <property name="toplink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="toplink.logging.level" value="SEVERE"/>
    </properties>   
  </persistence-unit>
</persistence>
